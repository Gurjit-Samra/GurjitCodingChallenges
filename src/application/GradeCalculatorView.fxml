                     <Integer fx:value="15" />
                  </FXCollections>
               </items>
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </ChoiceBox>
            <Button contentDisplay="CENTER" mnemonicParsing="false" onAction="#getQuizGrades" text="Enter Quiz Grades" translateY="50.0">
         		<VBox.margin>
            		<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         		</VBox.margin>
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
      		</Button>               
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </HBox>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label alignment="CENTER" text="# of optional quizzes completed   ">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Label>
            <ChoiceBox fx:id="optionalQuizzesChoiceBox" prefHeight="51.0" prefWidth="221.0">
               <items>
                  <FXCollections fx:factory="observableArrayList">
                     <Integer fx:value="0" />
                     <Integer fx:value="1" />
                     <Integer fx:value="2" />
                     <Integer fx:value="3" />
