<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.collections.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>
<?import javafx.collections.FXCollections?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.GradeCalculatorController">
   <children>
      <HBox>
         <children>
            <Label text="Project Grade" translateX="60.0" translateY="15.0">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin></Label>
            <TextField fx:id="projectGradeTextField" prefHeight="51.0" prefWidth="219.0" translateX="116.0">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin></TextField>
            <Label text="\%" translateX="100.0" translateY="15.0">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Label>
            <Label fx:id="projectErrorLabel" text="Display for Error Message" translateX="100.0" translateY="15.0">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Label>
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </HBox>
      <HBox>
         <children>
            <Label text="Required Coding Challenges Passed">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin></Label>
            <ChoiceBox fx:id="requiredCodingChallengesChoice" prefHeight="51.0" prefWidth="219.0" translateX="-3.0">
            	<items>
            		<FXCollections fx:factory="observableArrayList">
            			<Integer fx:value="0" />
            			<Integer fx:value="1" />
            			<Integer fx:value="2" />
            			<Integer fx:value="3" />
            			<Integer fx:value="4" />
            			<Integer fx:value="5" />
            			<Integer fx:value="6" />
            			<Integer fx:value="7" />
            			<Integer fx:value="8" />
            			<Integer fx:value="9" />
            			<Integer fx:value="10" />
            			<Integer fx:value="11" />
            			<Integer fx:value="12" />
            			<Integer fx:value="13" />
            			<Integer fx:value="14" />
            			<Integer fx:value="15" />
            		</FXCollections>
            	</items>
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </ChoiceBox>
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </HBox>
      <HBox>
         <children>
            <Label text="Optional Coding Challenges Passed">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Label>
            <ChoiceBox fx:id="optionalCodingChallengesChoice" prefHeight="51.0" prefWidth="221.0">
               <items>
                  <FXCollections fx:factory="observableArrayList">
                     <Integer fx:value="0" />
                     <Integer fx:value="1" />
                     <Integer fx:value="2" />
                     <Integer fx:value="3" />
                     <Integer fx:value="4" />
                     <Integer fx:value="5" />
                  </FXCollections>
               </items>
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </ChoiceBox>
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </HBox>
      <HBox>
         <children>
            <Label text="Average Quiz Grade   " translateX="45.0">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin></Label>
            <Slider fx:id="quizSlider" blockIncrement="0.5" majorTickUnit="1.0" max="10.0" minorTickCount="0" prefHeight="35.0" prefWidth="217.0" showTickLabels="true" showTickMarks="true" translateX="75.0">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin></Slider>
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </HBox>
      <Button contentDisplay="CENTER" mnemonicParsing="false" onAction="#calculateGrade" text="Calculate Grade" translateX="192.5">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </Button>
      <Label fx:id="courseGradeLabel" text="" />
   </children>
</VBox>
