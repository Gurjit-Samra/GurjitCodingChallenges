                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin></Label>
            <ChoiceBox fx:id="requiredCodingChallengesChoice" prefHeight="51.0" prefWidth="219.0" translateX="-3.0">
            	<items>
            		<FXCollections fx:factory="observableArrayList">
            			<Integer fx:value="0" />
            			<Integer fx:value="1" />
            			<Integer fx:value="2" />
